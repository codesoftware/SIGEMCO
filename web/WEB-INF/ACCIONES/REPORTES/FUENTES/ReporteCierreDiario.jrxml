	<subDataset name="facturas" uuid="8d7e6b73-ab06-474d-aeaf-e8574e3d95a3">
		<parameter name="CIER_CIER" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT fact_fact FACTURA ,tius_usuario,'$'||to_char((fact_vlr_total + fact_vlr_iva),'999,999,999.99') TOTAL,
CASE WHEN  FACT_TIPO_PAGO = 'E' THEN 'EFECTIVO'
ELSE 
'TARJETA'
END TIPOPAGO
FROM fa_tfact, us_ttius
where fact_tius = tius_tius
AND fact_cierre = $P!{CIER_CIER}]]>
		</queryString>
		<field name="factura" class="java.lang.Integer"/>
		<field name="tius_usuario" class="java.lang.String"/>
		<field name="total" class="java.lang.String"/>
		<field name="tipopago" class="java.lang.String"/>
	</subDataset>
	<parameter name="FECHA" class="java.lang.String"/>
	<parameter name="SEDE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT cier_cier CIERRE,sede_nombre NOMBRESEDE,
'$'||to_char(cier_vlrc,'999,999,999.99')total from em_tsede,ad_tcier 
where cier_fech = to_date($P{FECHA},'dd/mm/yyyy') and cier_sede =$P!{SEDE} and sede_sede = cier_sede;]]>
	</queryString>
	<field name="cierre" class="java.lang.Integer"/>
	<field name="nombresede" class="java.lang.String"/>
	<field name="total" class="java.lang.String"/>
