	<subDataset name="tabla" uuid="a6f4702e-da7a-4e20-bc15-fd59fbb03e09">
		<queryString language="SQL">
			<![CDATA[select dska_cod, dska_nom_prod, refe_desc, marca_nombre, cate_desc,
minimos.kapr_fecha fechaMin, minimos.kapr_cost_saldo_uni saldoUniMin, minimos.kapr_cost_saldo_tot saldoTotalMin, minimos.kapr_cant_saldo cantSaldoMin,
maximos.kapr_fecha, maximos.kapr_cost_saldo_uni, maximos.kapr_cost_saldo_tot, maximos.kapr_cant_saldo
from in_tdska, in_trefe, in_tmarca, in_tcate,
(select kapr_dska,kapr_fecha, kapr_cost_saldo_uni, kapr_cost_saldo_tot, kapr_cant_saldo
from in_tkapr pr1
where pr1.kapr_kapr = (select min(kapr_kapr) from in_tkapr pr2 where pr2.kapr_dska = pr1.kapr_dska)) minimos,
(select kapr_dska,kapr_fecha, kapr_cost_saldo_uni, kapr_cost_saldo_tot, kapr_cant_saldo
from in_tkapr pr1
where pr1.kapr_kapr = (select max(pr2.kapr_kapr) from in_tkapr pr2 where pr2.kapr_dska = pr1.kapr_dska)) maximos
where dska_refe = refe_refe
and marca_marca = dska_marca
and dska_cate = cate_cate
and minimos.kapr_dska = dska_dska
and maximos.kapr_dska = dska_dska]]>
		</queryString>
