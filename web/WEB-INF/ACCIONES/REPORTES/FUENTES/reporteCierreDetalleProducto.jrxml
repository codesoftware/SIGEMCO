	<parameter name="CIERRE" class="java.lang.String"/>
	<queryString>
		<![CDATA[select codigo,cantidades,to_char(valor_total ,'LFM9,999,999.00') valor_total ,producto.dska_nom_prod
from (select '1-'||dtpr_dska codigo, sum(dtpr_cant) cantidades,sum(dtpr_vlr_total) valor_total
from 
fa_tdtpr,fa_tfact,in_tdska
where fact_fact =  dtpr_fact 
and dska_dska = dtpr_dska 
and fact_cierre = cast($P{CIERRE} as int)
group by dtpr_dska ) agrupacion, in_tdska producto
where  agrupacion.codigo = producto.dska_cod
union
select codigo, cantidades,to_char(valor_total,'LFM9,999,999.00') valor_total,producto.rece_nombre
from (select '3-'||dtre_rece codigo, sum(dtre_cant)cantidades, sum(dtre_vlr_venta_tot) valor_total
from fa_tdtre,fa_tfact,in_trece
where fact_fact = dtre_fact 
and fact_cierre = cast($P{CIERRE} as int)
and rece_rece = dtre_rece
group by dtre_rece) agrupacion, in_trece producto
where agrupacion.codigo = producto.rece_codigo]]>
	</queryString>
