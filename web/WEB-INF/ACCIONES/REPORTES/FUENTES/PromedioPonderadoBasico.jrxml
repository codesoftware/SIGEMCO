	<subDataset name="kardex" uuid="dee1f0cc-2ed6-4c50-87e8-d31d9cbe55f9">
		<parameter name="DSKA1" class="java.lang.String"/>
		<parameter name="fechaIni" class="java.lang.String"/>
		<parameter name="fechaFin" class="java.lang.String"/>
		<queryString>
			<![CDATA[select kapr_cons_pro consec,                           
    to_char(kapr_fecha, 'dd/mm/yyyy') fecha,           
    mvin_descr mvInv,                                  
    case when mvin_natu = 'I' then  kapr_cant_mvto     
        end cantMvtoEn,                                
    case when mvin_natu = 'I' then  kapr_cost_mvto_uni 
        end costMvtoUniEn,                             
    case when mvin_natu = 'I' then  kapr_cost_mvto_tot 
        end costMvtTotEn,                              
    case when mvin_natu = 'E' then  kapr_cant_mvto     
        end cantMvtoSa,                                
    case when mvin_natu = 'E' then  kapr_cost_mvto_uni 
        end costMvtoUniSa,                             
    case when mvin_natu = 'E' then  kapr_cost_mvto_tot 
        end costMvtTotSa,                              
    kapr_cant_saldo cantSaldo,                         
    kapr_cost_saldo_uni,                               
    kapr_cost_saldo_tot,                                
    sede_nombre  sede                              
from in_tkapr, in_tmvin, em_tsede                      
where kapr_dska = cast($P{DSKA1} as int )                               
and kapr_mvin = mvin_mvin                              
and cast(kapr_fecha as date) between to_date(cast($P{fechaIni} as varchar), 'dd/mm/yyyy') and to_date(cast($P{fechaFin} as varchar), 'dd/mm/yyyy')
and kapr_sede = sede_sede    order by kapr_cons_pro asc]]>
		</queryString>
		<field name="consec" class="java.lang.Integer"/>
		<field name="fecha" class="java.lang.String"/>
		<field name="mvinv" class="java.lang.String"/>
		<field name="cantmvtoen" class="java.lang.Integer"/>
		<field name="costmvtounien" class="java.math.BigDecimal"/>
		<field name="costmvttoten" class="java.math.BigDecimal"/>
		<field name="cantmvtosa" class="java.lang.Integer"/>
		<field name="costmvtounisa" class="java.math.BigDecimal"/>
		<field name="costmvttotsa" class="java.math.BigDecimal"/>
		<field name="cantsaldo" class="java.lang.Integer"/>
		<field name="kapr_cost_saldo_uni" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Saldo en costos de cada producto en bodega(Promedio ponderado de cada producto)]]></fieldDescription>
		</field>
		<field name="kapr_cost_saldo_tot" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Saldo en costos de los productos existentes a la fecha por todos los productos en bodega]]></fieldDescription>
		</field>
		<field name="sede" class="java.lang.String"/>
	</subDataset>
	<parameter name="fechaFinal" class="java.lang.String"/>
	<parameter name="fechaInicial" class="java.lang.String"/>
	<parameter name="dska" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT (SELECT para_valor FROM em_tpara where UPPER(para_clave) = 'NIT') NIT,  
       (SELECT para_valor FROM em_tpara where UPPER(para_clave) = 'NOMBREEMPRESA') NOMBRE,
       (SELECT para_valor FROM em_tpara where UPPER(para_clave) = 'TELEFONOS') TELEFONOS,
       (SELECT para_valor FROM em_tpara where UPPER(para_clave) = 'DIRECCION') DIRECCION,
       (SELECT para_valor FROM em_tpara where UPPER(para_clave) = 'CIUDAD') CIUDAD,
       dska_cod, 
       dska_nom_prod
FROM IN_TDSKA
WHERE dska_dska = cast($P{dska} as int)]]>
	</queryString>
	<field name="nit" class="java.lang.String"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="telefonos" class="java.lang.String"/>
	<field name="direccion" class="java.lang.String"/>
	<field name="ciudad" class="java.lang.String"/>
	<field name="dska_cod" class="java.lang.String"/>
